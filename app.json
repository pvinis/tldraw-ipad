{
  "expo": {
    "name": "tldraw-ipad",
    "slug": "tldraw-ipad",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tldraw-ipad",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.pvinis.tldraw-ipad",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "34d17fcf-a1fa-4623-ae63-d5d132b3b1b6"
      }
    },
    "owner": "pvinis",
    "updates": {
      "url": "https://u.expo.dev/34d17fcf-a1fa-4623-ae63-d5d132b3b1b6"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
